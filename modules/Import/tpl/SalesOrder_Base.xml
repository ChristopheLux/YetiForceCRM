			<TaxID refmoule="Accounts" crmfieldtype="reference" crmfield="vat_id" refkeyfld="account_id"></TaxID> 
			<AccountNumber refmoule="Accounts" crmfieldtype="reference" crmfield="bank_account_number" refkeyfld="account_id"></AccountNumber> 
			<Name refmoule="Accounts" crmfieldtype="reference" crmfield="accountname" refkeyfld="account_id"></Name>
			<StreetAndNumber refmoule="Accounts" crmfieldtype="reference" crmfield="addresslevel8a" refkeyfld="account_id"></StreetAndNumber> 
			<CityName refmoule="Accounts" crmfieldtype="reference" crmfield="addresslevel5a" refkeyfld="account_id"></CityName> 
			<PostalCode refmoule="Accounts" crmfieldtype="reference" crmfield="addresslevel7a" refkeyfld="account_id"></PostalCode> 
