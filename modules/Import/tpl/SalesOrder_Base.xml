	</Order-Parties>
	
	<Order-Lines> 
		<Line type="product">
			<Line-Item notrepeat="true">
				<LineNumber notrepeat="true"></LineNumber> 
				<EAN notrepeat="true" crmfield="ean" frommodule="true" ></EAN> 
				<!--<BuyerItemCode notrepeat="true"></BuyerItemCode>--> 
				<SupplierItemCode notrepeat="true" crmfield="product_no|service_no" frommodule="true"></SupplierItemCode> 
				<PackageEAN notrepeat="true" crmfield="package_ean" frommodule="true"></PackageEAN> 
				<ItemDescription notrepeat="true" crmfield="productname|servicename" frommodule="true"></ItemDescription> 
				<!--<ItemType></ItemType>-->
				<OrderedQuantity notrepeat="true" crmfield="qty" alt="1"></OrderedQuantity> 
				<!--<FreeOrderedQuantity></FreeOrderedQuantity>-->
				<OrderedUnitPacksize notrepeat="true" crmfield="qty_per_unit" alt="" frommodule="true"></OrderedUnitPacksize>
				<InvoicedQuantity notrepeat="true" crmfield="qty" alt="1"></InvoicedQuantity> 
				<!--<UnitOfMeasure></UnitOfMeasure>-->
				<OrderedUnitNetPrice notrepeat="true" crmfield="hdnSubTotal" fromfinal="true"></OrderedUnitNetPrice> 
				<OrderedUnitRetailPrice notrepeat="true" crmfield="listPrice"></OrderedUnitRetailPrice> 
