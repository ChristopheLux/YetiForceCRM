	<Order-Parties> 
		<Buyer> 
			<ILN refmoule="Accounts" crmfieldtype="reference" crmfield="iln" refkeyfld="account_id"></ILN>  
			<TaxID refmoule="Accounts" crmfieldtype="reference" crmfield="vat_id" refkeyfld="account_id"></TaxID> 
			<AccountNumber refmoule="Accounts" crmfieldtype="reference" crmfield="bank_account_number" refkeyfld="account_id"></AccountNumber> 
			<Name refmoule="Accounts" crmfieldtype="reference" crmfield="accountname" refkeyfld="account_id"></Name>
			<StreetAndNumber crmfieldtype="string" crmfield="addresslevel8a buildingnumbera localnumbera" refkeyfld="account_id"></StreetAndNumber>  
			<CityName crmfieldtype="string" crmfield="addresslevel5a" refkeyfld="account_id"></CityName> 
			<PostalCode crmfieldtype="string" crmfield="addresslevel7a" refkeyfld="account_id"></PostalCode>   
			<Country crmfieldtype="string" crmfield="addresslevel1a" refkeyfld="account_id"></Country>  
			<CourtAndCapitalInformation></CourtAndCapitalInformation> 
			<ContactInformation></ContactInformation>
		</Buyer>
		<BuyerHeadquarters> 
			<ILN refmoule="Accounts" crmfieldtype="reference" crmfield="iln" refkeyfld="account_id"></ILN> 
			<Name refmoule="Accounts" crmfieldtype="reference" crmfield="accountname" refkeyfld="account_id"></Name> 
			<StreetAndNumber crmfieldtype="string" crmfield="addresslevel8b buildingnumberb localnumberb"></StreetAndNumber>  
			<CityName crmfieldtype="string" crmfield="addresslevel5b"></CityName> 
			<PostalCode crmfieldtype="string" crmfield="addresslevel7b"></PostalCode>  
			<Country crmfieldtype="string" crmfield="addresslevel1b"></Country>  
		</BuyerHeadquarters>
		<Invoicee>
			<ILN></ILN> 
			<TaxID></TaxID> 
			<Name></Name>
			<StreetAndNumber></StreetAndNumber> 
			<CityName></CityName> 
			<PostalCode></PostalCode> 
			<Country></Country>
		</Invoicee>
		<Seller>
			<ILN crmfieldtype="company" crmfield="iln"></ILN>
			<TaxID crmfieldtype="company" crmfield="vatid"></TaxID> 
			<AccountNumber crmfieldtype="company" crmfield="bank_account_number"></AccountNumber> 
			<Name crmfieldtype="company" crmfield="organizationname"></Name> 
			<StreetAndNumber crmfieldtype="company" crmfield="address"></StreetAndNumber> 
			<CityName crmfieldtype="company" crmfield="city"></CityName> 
			<PostalCode crmfieldtype="company" crmfield="code"></PostalCode> 
			<Country crmfieldtype="company" crmfield="country"></Country> 
			<CodeByBuyer></CodeByBuyer> 
			<Fax crmfieldtype="company" crmfield="fax"></Fax>
		</Seller>
