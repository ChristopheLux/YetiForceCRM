<Document-Order>
	<Order-Header> 
		<OrderNumber crmfield="salesorder_no" crmfieldtype="string"></OrderNumber> 
		<VendorOrderNumber></VendorOrderNumber> 
		<OrderDate crmfield="createdtime" crmfieldtype="date" partvalue="0"></OrderDate> 
		<ExpectedDeliveryDate></ExpectedDeliveryDate> 
		<ExpectedDeliveryTime></ExpectedDeliveryTime> 
		<CollectionDate></CollectionDate> 
		<CollectionTime></CollectionTime> 
		<PromotionReference></PromotionReference>	
		<DocumentFunctionCode constvalue="O"></DocumentFunctionCode>
		<MessageType constvalue="OD"></MessageType> 
		<Remarks crmfield="attention" crmfieldtype="string"></Remarks> 
		<OrderCurrency crmfield="currency_id" crmfieldtype="currency"></OrderCurrency> 
